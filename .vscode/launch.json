{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug [Internal] - Blank",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cli",
            "console": "integratedTerminal",
            "args": [
                "--help"
            ]
        },
        {
            "name": "Debug [Internal] - Sample brightness (test.png)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cli",
            "console": "integratedTerminal",
            "args": [
                "brightness",
                "--file-path", "test.png",
                "--output-format", "jpg",
                "--direction", "ascending",
                "--interval-determinant", "brightness",
                "--interval-lower-threshold", "0.15",
                "--interval-upper-threshold", "0.85",
                "--order", "horizontal-vertical",
                "--angle", "0"
            ]
        },
        {
            "name": "Debug [External] - Blank",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cli",
            "console": "externalTerminal",
            "args": [
                "--help"
            ]
        }       
    ]
}